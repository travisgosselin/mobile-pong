@using System.Web.Optimization
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1"> 
    @Styles.Render("~/Content/jquery.mobile-1.3.0.min.css")
    @Styles.Render("~/Content/toastr.min.css")
    @Styles.Render("~/Content/master.css")
</head>
<body>
    <div data-role="page" data-theme="b">
        <div data-role="header">
            <a data-role="button" href="@Url.Content("~/Home/")">Lobby</a>
            <h1>@ViewBag.Title</h1>
        </div>
        <div data-role="content">
             @RenderBody()
        </div>
    </div>
    
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/scripts/jquery-1.9.1.min.js");  
    @Scripts.Render("~/scripts/underscore.min.js") 
    <script type="text/javascript">
        $(document).bind("mobileinit", function () {
            $.mobile.ajaxEnabled = false;
        });
    </script>
    @Scripts.Render("~/scripts/jquery.mobile-1.3.0.min.js")
    @Scripts.Render("~/scripts/toastr.min.js")
    @RenderSection("scripts", required: false)
</body>
</html>
